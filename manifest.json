{
  "manifest_version": 3,
  "name": "Doom element",
  "description": "ctrl/cmd+alt click to shoot down dom nodes. ctrl/cmd+z if you miss",
  "version": "1.0",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "permissions": ["scripting", "tabs"],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  }
}
